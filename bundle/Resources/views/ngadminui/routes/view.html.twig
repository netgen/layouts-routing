{% extends 'NetgenAdminUIBundle::pagelayout.html.twig' %}

{% trans_default_domain 'routing_admin' %}

{% block stylesheets %}
    {{ parent() }}
    {#    <link rel="stylesheet" type="text/css" href="{{ asset('bundles/shopadmin/css/style.css') }}" />#}
{% endblock %}

{% block content %}
    <div class="main-content">
        <div id="path">
            <div id="path-design">
                <p class="path">
                    <span class="path-here-text">{{ 'netgen_admin_ui.layout.path.you_are_here'|trans({}, 'messages') }}</span>
                    <span class="path">{{ 'title'|trans }} / {{ 'route_view.title'|trans }}</span>
                </p>
            </div>
        </div>

        <div class="box-header">
            <h1 class="context-title">{{ 'route_view.title'|trans }}</h1>
        </div>

        <div class="panel">
            <p>{{ app.request.pathInfo }}</p>
            <p>{{ path( 'nglayouts_routing_admin_dashboard' ) }}</p>
        </div>
    </div>
{% endblock %}
